{
    "name": "Line Forty Nine",
    "description": "Extension to show bus arrival time and map",
    "version": "1.0",
    "manifest_version": 3,
    
    "action": {
      "default_popup": "/popup/popup.html",
      "default_icon": "/images/LineFortyNineLogo.png"
    },
      
    "background":{
      "service_worker": "background.js",
      "type": "module"
    },
    "permissions":[
      "storage",
      "alarms",
      "notifications"
    ]
  }